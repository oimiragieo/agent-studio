{
  "name": "agent-studio",
  "version": "2.0.0",
  "description": "Multi-platform agent configuration bundle for Claude Code, Cursor IDE, and Factory Droid",
  "type": "module",
  "private": true,
  "scripts": {
    "validate": "node scripts/validate-config.mjs && node scripts/validate-model-names.mjs",
    "validate:full": "pnpm validate && node scripts/validate-workflow.mjs && node scripts/validate-all-references.mjs && node scripts/validate-cujs.mjs && pnpm validate:index && pnpm index-rules",
    "validate:verbose": "node scripts/validate-config.mjs --verbose && node scripts/validate-model-names.mjs --verbose",
    "validate:sync": "bash scripts/validate-sync.sh",
    "validate:models": "node scripts/validate-model-names.mjs",
    "validate:workflow": "node scripts/validate-workflow.mjs",
    "validate:references": "node scripts/validate-all-references.mjs",
    "validate:cujs": "node scripts/validate-cujs.mjs",
    "validate:cuj": "node .claude/tools/validate-cuj.mjs",
    "validate:cujs:e2e": "node .claude/tools/validate-cuj-e2e.mjs",
    "validate:cujs:dry-run": "node scripts/validate-cuj-dry-run.mjs",
    "validate:index": "node scripts/validate-index.mjs",
    "validate:index-version": "node scripts/validate-rule-index-paths.mjs --check-version",
    "validate:index-paths": "node scripts/validate-rule-index-paths.mjs",
    "validate:all": "pnpm validate:full",
    "test:version-validation": "node scripts/test-version-validation.mjs",
    "install-agents": "node scripts/install.mjs",
    "index-rules": "node scripts/generate-rule-index.mjs",
    "index-rules:prebuilt": "node scripts/generate-prebuilt-rule-index.mjs",
    "sync-cuj-registry": "node .claude/tools/sync-cuj-registry.mjs",
    "sync-cuj-registry:validate": "node .claude/tools/sync-cuj-registry.mjs --validate-only",
    "cuj": "node .claude/tools/run-cuj.mjs",
    "cuj:list": "node .claude/tools/run-cuj.mjs --list",
    "cuj:simulate": "node .claude/tools/run-cuj.mjs --simulate",
    "cuj:validate": "node .claude/tools/run-cuj.mjs --validate",
    "cuj:doctor": "node .claude/tools/cuj-doctor.mjs",
    "cuj:doctor:json": "node .claude/tools/cuj-doctor.mjs --json",
    "validate:workflow-gates": "node .claude/tools/validate-workflow-rating-gates.mjs"
  },
  "keywords": [
    "claude",
    "cursor",
    "factory",
    "ai",
    "agents",
    "llm",
    "rules"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "ajv-formats": "^3.0.1",
    "glob": "^13.0.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "ajv": "^8.12.0",
    "js-yaml": "^4.1.0"
  }
}
