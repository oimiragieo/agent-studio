{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Primary Agent Artifacts",
  "description": "Schema for primary-agent-artifacts produced during fallback routing tests.",
  "type": "object",
  "required": ["workflow_id", "generated_at", "agent", "artifacts"],
  "properties": {
    "workflow_id": { "type": "string" },
    "generated_at": { "type": "string", "format": "date-time" },
    "agent": { "type": "string" },
    "artifacts": { "type": "array", "items": { "type": "string" } },
    "notes": { "type": "string" }
  },
  "additionalProperties": true
}
